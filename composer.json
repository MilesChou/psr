{
    "name": "mileschou/psr",
    "description": "The support library for PSR",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "psr"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "provide": {
        "psr/http-factory-implementation": "1.0"
    },
    "require": {
        "php": "^7.4 | ^8.0",
        "psr/container": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "psr/simple-cache": "^2.0 | ^3.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Require ^7 if need use PSR-18",
        "guzzlehttp/psr7": "Require ^2 if need use PSR-7 and PSR-17"
    },
    "require-dev": {
        "ext-json": "*",
        "guzzlehttp/psr7": "^1|^2",
        "http-interop/http-factory-guzzle": "^1.0",
        "http-interop/http-factory-slim": "^2.0",
        "laminas/laminas-diactoros": "^2.0",
        "nyholm/psr7": "^1.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Psr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
